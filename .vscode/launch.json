{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "start:bot",
            "type": "node",
            "request": "launch",
            "protocol": "inspector",
            "preLaunchTask": "build",
            "cwd": "${workspaceFolder}",
            "runtimeExecutable": "node",
            "args": ["--enable-source-maps", "${workspaceFolder}/dist/index.js"],
            "resolveSourceMapLocations": ["${workspaceFolder}/**", "!**/node_modules/**"],
            "outputCapture": "std",
            "internalConsoleOptions": "openOnSessionStart",
            "skipFiles": ["<node_internals>/**"],
            "restart": false
        },
        {
            "name": "start:manager",
            "type": "node",
            "request": "launch",
            "protocol": "inspector",
